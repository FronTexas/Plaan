<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.plaan"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_plaan"
        android:label="@string/app_name" >
        <activity
            android:name="com.example.plaan.FirstPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.plaan.WtdwsPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.plaan.WTDWSPAGE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TodoPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.plaan.TODOPAGE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MinutesLeftPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.plaan.MINUTESLEFTPAGE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SetTimePage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.plaan.SETTIMEPAGE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WtdwsClock"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Holo.Dialog" >
            <intent-filter>
                <action android:name="com.example.plaan.WTDWSCLOCK" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TimeTypeSpinner"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.plaan.SCA" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivityTransition"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.plaan.MAT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NextActivityTransition"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.plaan.NAT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SummaryPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.plaan.SUMMARYPAGE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityCountDownPage"
            android:label="@string/app_name"
            android:launchMode="singleTop" >
            <intent-filter>
                <action android:name="com.example.plaan.ACDP" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScrollingUI"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:windowSoftInputMode="adjustPan" >
            <intent-filter>
                <action android:name=".ScrollingUI" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Transition_animation_example"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name=".tae" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>